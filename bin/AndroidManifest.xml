<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.danielburgnerjr.flipulator"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
			android:name=".WhatIsFlipulator"
			android:label="What Is A Flipulator" >
		</activity>
        <activity
			android:name=".Credits"
			android:label="Credits" >
		</activity>
        <activity
			android:name=".LocationActivity"
			android:label="Location" >
		</activity>
        <activity
			android:name=".SalesMortgageActivity"
			android:label="Sales/Mortgage Info" >
		</activity>
        <activity
			android:name=".RehabActivity"
			android:label="Rehab" >
		</activity>
        <activity
			android:name=".ReservesActivity"
			android:label="Reserves" >
		</activity>
        <activity
			android:name=".ClosExpPropMktInfoActivity"
			android:label="Closing Expenses/Prop Mkt Info" >
		</activity>
        <activity
			android:name=".FinalResultActivity"
			android:label="Final Result" >
		</activity>
        
    </application>

</manifest>
