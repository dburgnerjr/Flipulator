<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <ScrollView
        android:id="@+id/scvFinalResult"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content" >
	    
        <LinearLayout 
            android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="vertical" >        
	    
		    <LinearLayout
		        android:id="@+id/llLocationFR"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:layout_marginTop="10dp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtLocation"
			        android:layout_width="0dp"
			        android:layout_height="20sp"        
			        android:layout_weight="1"
			        android:text="@string/location" />        
		    </LinearLayout>
		 
		    <LinearLayout
		        android:id="@+id/llLocAddress"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtLocationAddress"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/address" />      	               
		    </LinearLayout>
		
		    <LinearLayout
		        android:id="@+id/llLocCityStZip"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtLocationCityStZip"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/citystzip" />	               
		    </LinearLayout>
		    
		    
		    <LinearLayout
		        android:id="@+id/llLocSquareFootage"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtLocationSqFootage"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/sq_footage" />	               
		    </LinearLayout>
		        
		    <LinearLayout
		        android:id="@+id/llLocBedBath"
		        android:layout_width="fill_parent"
		        android:layout_height="35sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtLocationBedBath"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/bed_bath" />	               
		    </LinearLayout>
		        
		    <LinearLayout
		        android:id="@+id/llSalesInfoFR"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtSalesInfo"
			        android:layout_width="0dp"
			        android:layout_height="20sp"        
			        android:layout_weight="1"
			        android:text="@string/sales" />        
		    </LinearLayout>
		    
		    <LinearLayout
		        android:id="@+id/llSalesPrice"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtSalePrice"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/salePrice" />	               
		    </LinearLayout>
		    
		     <LinearLayout
		        android:id="@+id/llPercentDown"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtPercentDown"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/percentDown" />	               
		    </LinearLayout>
		 
		     <LinearLayout
		        android:id="@+id/llOfferBid"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtOfferBid"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/offerBid" />	               
		    </LinearLayout>
		    
		     <LinearLayout
		        android:id="@+id/llRehabBudget"
		        android:layout_width="fill_parent"
		        android:layout_height="35sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtRehabBudget"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/rehab_budget" />	               
		    </LinearLayout>
		
		    <LinearLayout
		        android:id="@+id/llMortgageFR"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtMortgageInfo"
			        android:layout_width="0dp"
			        android:layout_height="20sp"        
			        android:layout_weight="1"
			        android:text="@string/mortgage" />        
		    </LinearLayout>
		
		    <LinearLayout
		        android:id="@+id/llDownPayment"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtDownPayment"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/downPayment" />	               
		    </LinearLayout>
		    
		    <LinearLayout
		        android:id="@+id/llLoanAmount"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtLoanAmount"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/loanAmount" />	               
		    </LinearLayout>
		    
		    <LinearLayout
		        android:id="@+id/llInterestRate"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtInterestRate"
			        android:layout_width="0dp"
			        android:layout_height="20sp"        
			        android:layout_weight="1"
			        android:text="@string/interestRate" />        
		    </LinearLayout>
		
		    <LinearLayout
		        android:id="@+id/llTerm"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtTerm"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/term" />	               
		    </LinearLayout>
		    
		    <LinearLayout
		        android:id="@+id/llMonthlyPmt"
		        android:layout_width="fill_parent"
		        android:layout_height="35sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtMonthlyPmt"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/monthlyPmt" />	               
		    </LinearLayout>
		    
		    <LinearLayout
		        android:id="@+id/llReservesFR"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtReserves"
			        android:layout_width="0dp"
			        android:layout_height="20sp"        
			        android:layout_weight="1"
			        android:text="@string/reserves" />        
		    </LinearLayout>
		
		    <LinearLayout
		        android:id="@+id/llMortPmt"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtMortPmt"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/mortPmt" />	               
		    </LinearLayout>
		    
		    <LinearLayout
		        android:id="@+id/llInsurance"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtInsurance"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/insurance" />	               
		    </LinearLayout>
		    
		    <LinearLayout
		        android:id="@+id/llPropertyTaxes"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtPropertyTaxes"
			        android:layout_width="0dp"
			        android:layout_height="20sp"        
			        android:layout_weight="1"
			        android:text="@string/taxes" />        
		    </LinearLayout>
		
		    <LinearLayout
		        android:id="@+id/llElectric"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtElectric"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/electric" />	               
		    </LinearLayout>
		    
		    <LinearLayout
		        android:id="@+id/llGas"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtGas"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/gas" />	               
		    </LinearLayout>
		    
		    <LinearLayout
		        android:id="@+id/llWater"
		        android:layout_width="fill_parent"
		        android:layout_height="20sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtWater"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/water" />	               
		    </LinearLayout>

		    <LinearLayout
		        android:id="@+id/llTotalReserves"
		        android:layout_width="fill_parent"
		        android:layout_height="100sp"
		        android:orientation="horizontal" >
		
		        <TextView 
			        android:id="@+id/txtTotalReserves"
			        android:layout_width="0dp"
			        android:layout_height="20sp"
			        android:layout_weight="1"
			        android:text="@string/totres" />	               
		    </LinearLayout>
		    		    
		</LinearLayout>
    
	</ScrollView>
	
    <LinearLayout
        android:id="@+id/llPrevSave"
        android:layout_width="fill_parent"
        android:layout_height="35sp"
        android:layout_above="@+id/llResEmail"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/btnPrevious"
            android:layout_width="100sp"
            android:layout_height="35sp"
            android:layout_weight="0.5"
            android:text="@string/prev"
            android:onClick="prevPage"
            android:background="#690"
            android:textColor="#ffffff" />

        <Button
            android:id="@+id/btnSave"
            android:layout_width="100sp"
            android:layout_height="35sp"
            android:layout_weight="0.5"
            android:text="@string/save"
            android:background="#690"
            android:textColor="#ffffff" />
    </LinearLayout>
	
	<LinearLayout
        android:id="@+id/llResEmail"
        android:layout_width="fill_parent"
        android:layout_height="35sp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/btnMainMenu"
            android:layout_width="100sp"
            android:layout_height="35sp"
            android:layout_weight="0.5"
            android:onClick="mainMenu"
            android:text="@string/main_menu"
            android:background="#690"
            android:textColor="#ffffff" />

        <Button
            android:id="@+id/btnEmail"
            android:layout_width="100sp"
            android:layout_height="35sp"
            android:layout_weight="0.5"
            android:onClick="nextPage"
            android:text="@string/email"
            android:background="#690"
            android:textColor="#ffffff" />
    </LinearLayout>
	
</RelativeLayout>